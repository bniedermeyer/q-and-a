{
  "name": "@cascadiajs/q-and-a",
  "version": "0.0.2",
  "description": "A web component to manage audience questions during a live streamed event",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/q-and-a/q-and-a.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "commit": "cz",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "homepage": "https://cascadiajs.com/",
  "author": {
    "name": "Brenden Niedermeyer",
    "email": "contact@brenden.dev"
  },
  "keywords": [
    "web component",
    "cascadia",
    "question",
    "answer",
    "question and answer"
  ],
  "dependencies": {
    "@stencil/core": "^2.3.0",
    "@stencil/store": "^1.3.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@jedmao/semantic-release-npm-github-config": "^1.0.9",
    "@types/jest": "^26.0.15",
    "cz-conventional-changelog": "^3.3.0",
    "fetch-mock-jest": "^1.3.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "prettier": "2.1.2",
    "pretty-quick": "^3.1.0",
    "semantic-release": "^17.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
